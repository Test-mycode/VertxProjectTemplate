{
  "configuration":{
    "status":"error",
    "name":"RoutingTest",
    "packages":"org.apache.logging.log4j.test",
    "properties":{
      "property":{
        "name":"filename",
        "value":"target/rolling1/rollingtest-$${sd:type}.log"
      }
    },
    "ThresholdFilter":{
      "level":"trace"
    },
    "appenders":{
      "appender":[
        {
          "type":"Console",
          "name":"STDOUT",
          "PatternLayout":{
            "pattern":"%d{HH:mm:ss.SSS} %-5level %class{36} %L %M - %msg%xEx%n"
          },
          "ThresholdFilter":{
            "level":"trace"
          }
        }
      ]
    },
    "loggers":{
      "logger":[
        {
          "name":"io.netty",
          "level":"error",
          "additivity":"false",
          "AppenderRef":{
            "ref":"STDOUT"
          }
        },
        {
          "name":"com.pqitech",
          "level":"trace",
          "additivity":"false",
          "AppenderRef":{
            "ref":"STDOUT"
          }
        },
        {
          "name":"com.mchange",
          "level":"error",
          "additivity":"false",
          "AppenderRef":{
            "ref":"STDOUT"
          }
        }
      ],
      "root":{
        "level":"trace",
        "AppenderRef":{
          "ref":"STDOUT"
        }
      }
    }
  }
}